<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.LeftMenuBar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TryingOut"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity
            android:name=".LoginFiles.LoginMenuItem.ChatListActivity"
            android:exported="false" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.ChatActivity"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.YourFriendLogin"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".MenuItems.ForgotPassFragment"
            android:exported="false" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.FindFriendPorfileLogin"
            android:exported="false" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.FindFriendLogin"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".LoginFiles.Admin.CreateUserAdmin"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.NewsLogin"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.SetupLogin"
            android:exported="false" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.ContactLogin"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.SettingsLogin"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".LoginFiles.LoginMenuItem.DashboardLogin"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".LoginFiles.LoginActivity"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".backcode.MyFirebaseMessagingService"
            android:exported="false"
            android:directBootAware="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>